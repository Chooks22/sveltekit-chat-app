{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"ES2021",
			"DOM"
		],
		"moduleResolution": "Node",
		"module": "ESNext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
		"paths": {
			"@components/*": [
				"./src/components/*"
			]
		}
	}
}